{ 
"plugins": ["plugins/markdown"], 
"recurseDepth": 10, 
"source": { 
	"include": ["app"], 
	"includePattern": ".+\\.(js|html)$",
	"excludePattern": "(node_modules/|docs)"
}, 
"templates": { 
	"cleverLinks": true, 
	"monospaceLinks": true
}, 
"opts": { 
	"destination": "./documentation", 
	"recurse": true, 
	"readme": "./README.md"
} 
} 
